<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Agents_cannot_put_Account_as_Contracted</fullName>
    <active>true</active>
    <errorConditionFormula>AND($Profile.Id = &apos;00ef4000001Qcr6&apos;, ISCHANGED(Status__c),
				ISPICKVAL(Status__c, &apos;Contracted&apos;))</errorConditionFormula>
    <errorMessage>Please ask Operations or Sys admin to set the Account to contracted.</errorMessage>
</ValidationRule>
